---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Ivo Jimenez</title>
 <link href="http://ivotron.github.com/atom.xml" rel="self"/>
 <link href="http://ivotron.github.com"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://cs.ucsc.edu/~ivo</id>
 <author>
   <name>Ivo Jimenez</name>
   <email>ivo@cs.ucsc.edu</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://ivotron.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ivotron.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>

